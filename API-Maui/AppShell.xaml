<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="API_Maui.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:API_Maui.Views"
    Shell.FlyoutBehavior="Disabled"
    Title="API_Maui">

    <TabBar>
        <ShellContent
            Title="Post"
            ContentTemplate="{DataTemplate local:PostView}"
            Route="MainPage" />
        <ShellContent
            Title="Todo"
            ContentTemplate="{DataTemplate local:TodoView}"
            Route="MainPage" />
    </TabBar>


</Shell>
